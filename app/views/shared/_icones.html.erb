      <% else %>
        <%= cl_image_tag(@default_pic, class: "mb-4", style: "height:160px; object-position: center center; object-fit: cover; width: 100%") %>
      <% end %>
    </div>
    <div class="col-6 col-md-2">
      <% if @page.square_pic.attached? %>
        <%= cl_image_tag(@page.square_pic.key, class: "mb-4", style: "height:160px; object-position: center center; object-fit: cover; width: 100%") %>
        <p><%= @page.benefit4 %></p>
      <% else %>
        <%= cl_image_tag(@default_pic, class: "mb-4", style: "height:160px; object-position: center center; object-fit: cover; width: 100%") %>
      <% end %>
    </div>
    <div class="col-6 col-md-2">
      <% if @page.title2.attached? %>
        <%= cl_image_tag(@page.title2.key, class: "mb-4", style: "height:160px; object-position: center center; object-fit: cover; width: 100%") %>
